/* pages/personal/personal.wxss */
.container{
  width: 100%;
  height: 100vh;
  background-color: #f8f8f8;
}
.top{
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  background-image: linear-gradient(to top, #fae0a7, #f4ba49);
}
image{
width: 150rpx;
height: 150rpx;
margin: 50rpx;
border-radius: 50%;
}
.right{
display: flex;
flex-direction: column;
}
.txt{
  color: red;
}
navigator{
  width: 100%;
  margin: 5rpx;
  background-color: #fff;
}
navigator image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.mar{
  margin:10rpx 0 10rpx 0;
}
